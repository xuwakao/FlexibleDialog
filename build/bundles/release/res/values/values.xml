    <style name="Mixture" />

    <style name="Mixture.Button">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:paddingTop">@dimen/grid_3</item>
        <item name="android:paddingBottom">@dimen/grid_3</item>
        <item name="android:minHeight">48dip</item>
        <item name="android:minWidth">64dip</item>
        <item name="android:paddingLeft">4dip</item>
        <item name="android:paddingRight">4dip</item>
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:textSize">15sp</item>
        <item name="android:textStyle">normal</item>
        <item name="android:layout_gravity">center_vertical</item>
    </style>

    <style name="Mixture.ButtonSeparator">
        <item name="android:layout_width">1dp</item>
        <item name="android:layout_height">match_parent</item>
    </style>

    <style name="Mixture.Dialog" parent="android:Theme">
        <item name="android:windowFrame">@null</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowSoftInputMode">stateUnspecified|adjustPan</item>
        <item name="android:backgroundDimAmount">0.6</item>
        <item name="android:colorBackgroundCacheHint">@null</item>
        <item name="mixtureDialogStyle">@style/Mixture.Dialog.DialogStyleLight</item>
    </style>

    <style name="Mixture.Dialog.ContentView">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:orientation">vertical</item>
    </style>

    <style name="Mixture.Dialog.DialogStyleLight">
        <item name="dialogBackground">@drawable/sdl_background_light</item>
        <item name="titleTextColor">@color/sdl_title_text_light</item>
        <item name="titleSeparatorColor">@color/sdl_title_separator_light</item>
        <item name="messageTextColor">@color/sdl_message_text_light</item>
        <item name="buttonTextColor">@color/sdl_button_text_light</item>
        <item name="buttonSeparatorColor">@color/sdl_button_separator_light</item>
        <item name="buttonBackgroundColorNormal">@color/sdl_button_normal_light</item>
        <item name="buttonBackgroundColorPressed">@color/sdl_button_pressed_light</item>
        <item name="buttonBackgroundColorFocused">@color/sdl_button_focused_light</item>
        <item name="actionButtonColorOrDrawable">true</item>
        <item name="buttonBackgroundDrawableNormal">@drawable/ic_action_bar_bg_bitmap</item>
        <item name="buttonBackgroundDrawablePressed">@drawable/ic_action_bar_bg_bitmap</item>
        <item name="buttonBackgroundDrawableFocused">@drawable/ic_action_bar_bg_bitmap</item>
        <!-- <item name="titleIcon">@drawable/ic_launcher</item> -->
        <!-- <item name="titleCloseIcon">@drawable/action_help_2</item> -->
    </style>

    <style name="Mixture.Dialog.TitleSeparator">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">2dp</item>
    </style>

    <style name="Mixture.Group">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:orientation">vertical</item>
    </style>

    <style name="Mixture.Group.ButtonPanel">
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="Mixture.Group.Content">
        <item name="android:layout_height">0dp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:gravity">center</item>
    </style>

    <style name="Mixture.Group.Horizontal">
        <item name="android:orientation">horizontal</item>
    </style>

    <style name="Mixture.Group.Horizontal.ButtonPanel">
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="Mixture.Group.Wrap">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_weight">1</item>
    </style>

    <style name="Mixture.HorizontalSeparator">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">1dp</item>
    </style>

    <style name="Mixture.ImageView">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_margin">@dimen/grid_1</item>
    </style>

    <style name="Mixture.Progress" parent="@android:style/Widget.ProgressBar">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginRight">0dp</item>
        <item name="android:layout_marginLeft">@dimen/grid_4</item>
        <item name="android:layout_marginTop">@dimen/grid_4</item>
        <item name="android:layout_marginBottom">@dimen/grid_4</item>
    </style>

    <style name="Mixture.RelativeLayout">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:gravity">center_vertical</item>
    </style>

    <style name="Mixture.TextView" parent="android:style/Widget.TextView">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">wrap_content</item>
    </style>

    <style name="Mixture.TextView.Message">
        <item name="android:textSize">16sp</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:paddingLeft">@dimen/grid_4</item>
        <item name="android:paddingRight">@dimen/grid_4</item>
        <item name="android:paddingTop">@dimen/grid_4</item>
        <item name="android:paddingBottom">@dimen/grid_4</item>
    </style>

    <style name="Mixture.TextView.Title">
        <item name="android:textSize">22sp</item>
        <item name="android:padding">@dimen/grid_1</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_marginLeft">8dp</item>
        <item name="android:layout_centerVertical">true</item>
    </style>

